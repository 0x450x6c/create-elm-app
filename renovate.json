{
  "extends": ["config:base"],
  "schedule": [
    "after 10pm every weekday",
    "before 5am every weekday",
    "every weekend"
  ],
  "commitMessagePrefix": "fix(package.json): ",
  "packageRules": [
    {
      "updateTypes": ["patch"],
      "commitMessagePrefix": "fix(package.json): ",
      "automerge": true
    },
    {
      "depTypeList": ["devDependencies"],
      "commitMessagePrefix": "chore(package.json): ",
      "automerge": true
    }
  ]
}
